action_deploy_CustomResourceDefinition_apiextensions_k8s_io()
{
  local res
  if [ -z "$DRYRUN" ]; then
    while [ "$res" != True ]; do
      res="$(kubectl --kubeconfig "$3" get crd "$1" -o jsonpath='{..status.conditions[?(@.type=="Established")].status}')"
      if [ "$res" != True ]; then
        info "waiting for resource being established"
        sleep 1
      fi
    done
  fi
}
